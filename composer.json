{
    "name": "gsdevme/jumpcloud",
    "description": "Simple library to authenicate with Jumpclouds REST Authorization API.",
    "license": "MIT License",
    "autoload": {
        "psr-4": {
            "JumpCloud\\": [
                "src/JumpCloud"
            ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "^1.0",
        "webmozart/json": "~1.2",
        "doctrine/collections": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^2.6"
    }
}
